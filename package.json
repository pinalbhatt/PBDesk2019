{
  "name": "pbdesk2019",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "node server.js -p $PORT",
    "build": "next build",
    "export": "next export",
    "generate": "npm run build && npm run export",
    "nojekyll": "touch out/.nojekyll",
    "cname": "touch out/CNAME && echo \"2019.pbdesk.com\" >> out/CNAME",
    "sfiles": "npm run nojekyll && npm run cname",
    "commit-out": "git add out/ && git commit -m \"Deploy to gh-pages\"",
    "push-out": "git subtree push --prefix out origin gh-pages",
    "git-out": "npm run commit-out && npm run push-out",
    "deploy": "npm run generate && npm run sfiles && npm run git-out",
    "start": "NODE_ENV=production node server.js -p $PORT",
    "test": "snyk test",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "engines": {
    "node": "8.11.4",
    "npm": "5.6.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
